<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Sample">
    <insert id="insert" parameterType="Sample">
        insert into Sample(obj1, obj2, obj3, objFile)
        values(#{obj1}, #{obj2}, #{obj3}, #{objFile})
    </insert>
     <select id="select" resultType="Sample">
        select * from Sample
    </select>
    <select id="byObjNum" parameterType="int" resultType="Sample">
    	select * from Sample where objNum = #{ObjNum}
    </select>
    <update id="update" parameterType="Sample">
    	 update Sample set obj1 = #{obj1}, obj2 = #{obj2}, obj3 = #{obj3}, objFile = #{objFile} where objNum = #{objNum}
    </update>
    <delete id="delete" parameterType="int">
        DELETE FROM Sample WHERE objNum = #{ObjNum} 
    </delete>
</mapper>

